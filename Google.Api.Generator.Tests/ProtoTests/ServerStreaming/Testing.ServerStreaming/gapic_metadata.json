{
  "schema": "1.0",
  "comment": "This file maps proto services/RPCs to the corresponding library clients/methods",
  "language": "csharp",
  "protoPackage": "testing.serverstreaming",
  "libraryPackage": "Testing.ServerStreaming",
  "services": {
    "ServerStreaming": {
      "clients": {
        "grpc": {
          "libraryClient": "Testing.ServerStreaming.ServerStreamingClientImpl",
          "rpcs": {
            "SignatureMethod": {
              "methods": [
                "SignatureMethod",
                "SignatureMethodAsync"
              ]
            },
            "ResourcedMethod": {
              "methods": [
                "ResourcedMethod",
                "ResourcedMethodAsync"
              ]
            }
          }
        }
      }
    }
  }
}
